        "\n",
        "plt.plot(x,x, 'r--' , label= \"linear\") #putting - creates lines \n",
        "plt.plot(x,x**2, 'bs' , label= \"squared\") #putting an s after any color creates squares\n",
        "plt.plot(x,x*3, 'g^', label= \"cubed\"); #^ creates triangles\n",
        "plt.legend();"
